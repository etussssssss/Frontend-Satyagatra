<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragmentgetstarted.NextPageThree">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.23">

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="wrap_content"
            android:layout_height="200dp"
            android:padding="10dp"
            android:src="@drawable/img_getstarted_pagethree" />

        <TextView
            android:layout_width="275dp"
            android:layout_height="60dp"
            android:fontFamily="@font/poppinsregular"
            android:gravity="center"
            android:text="Privasi Terjamin aman &amp; Terjaga"
            android:textColor="@color/textijofigma"
            android:textSize="18dp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="307dp"
            android:layout_height="79dp"
            android:fontFamily="@font/poppinsregular"
            android:gravity="center"
            android:text="Kerahasiaan ceritamu dijaga sepenuhnya dibawah Kode Etik Psikolog"
            android:textColor="@color/textgreyfigma"
            android:textSize="12dp"
            android:textStyle="bold" />

    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>